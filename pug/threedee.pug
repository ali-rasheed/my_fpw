//- ////////MIXINS/////////
mixin threedee-lazyloaded-lg(name1,name2,caption,cname)
  a(class=cname+" img-cont element-item p-tilt" data-groups=name1 data-lightbox="threedee" data-title=caption href="imgs/threedee/"+name2)  
    img(class=" lazy grid-image", data-src="imgs/threedee-sm/"+name1 ,   
    src="data:image/svg+xml,%3Csvg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 3 2'%3E%3C/svg%3E"
     ) 
    //-  src="imgs/threedee-sm/"+name1

doctype  
html(lang='en')
  head
    include ./includes/head1.pug
    title 3d visualisations - Ali Rasheed
  body 
    header.gallery
      include ./includes/nav1.pug  


    main.gallery 

          
      section.threedee-grid#threedeeCont
        //- +threedee-lazyloaded-lg("bed.jpg","bed.jpg","","w-reg") /
        +threedee-lazyloaded-lg("bed.jpg","bed.jpg","","w-triple") 
        +threedee-lazyloaded-lg("wtrmln.png","wtrmln.gif","Procedurally generated and textured watermelon","w-double")
        +threedee-lazyloaded-lg("Curator.png","Curator.png","Procedurally-generated Nebula full of star-dust!","w-reg")
        +threedee-lazyloaded-lg("nebula.png","nebula.png","Procedurally-generated Nebula full of star-dust!","w-reg")  
        +threedee-lazyloaded-lg("dancer.png","dancer.png","","w-reg")  
        //- +threedee-lazyloaded-lg("wtrmln.png","wtrmln.gif")
          
    footer
      include ./includes/footer2.pug