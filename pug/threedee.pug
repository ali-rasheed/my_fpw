mixin threedee-lazyloaded-lg(name1,name2,caption,cname,type)
  a(class=cname+" mfp-"+type+"   l-threedee img-cont p-tilt" href=name2 title=caption)  
    img(class=" lazy grid-image", data-src="imgs/threedee-sm/"+name1 ,   
    src="data:image/svg+xml,%3Csvg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 3 2'%3E%3C/svg%3E"
     ) 
    //-  src="imgs/threedee-sm/"+name1
 
doctype  
html(lang='en')
  head
    include ./includes/head1.pug
    title 3d visualisations - Ali Rasheed 
  body.threedee.threedee-proj 
    - var home = "/"

    header.gallery
      include ./includes/nav2.pug   

 
    main.gallery 
 
          
      section.threedee-grid#threedeeCont 
        //- +threedee-lazyloaded-lg("bed.jpg","bed.jpg","","w-reg") /
        +threedee-lazyloaded-lg("tet.jpg","https://vimeo.com/568222219","","w-double","iframe") 

        +threedee-lazyloaded-lg("nevarro.jpg","imgs/threedee/nevarro.jpg","","w-reg","image") 
        +threedee-lazyloaded-lg("openingsequence.png","imgs/threedee/openingsequence.mp4","","w-double","iframe")  

        +threedee-lazyloaded-lg("bed.jpg","imgs/threedee/bed.jpg","","w-double","image")  

        +threedee-lazyloaded-lg("wtrmln.jpg","imgs/threedee/wtrmln.gif","Procedurally generated and textured watermelon","w-reg","image") 

        +threedee-lazyloaded-lg("Curator.jpg","imgs/threedee/Curator.png","Procedurally-generated Nebula full of star-dust!","w-reg","image")
        
        +threedee-lazyloaded-lg("nebula.jpg","imgs/threedee/nebula.png","Procedurally-generated Nebula full of star-dust!","w-reg","image")  
        
        +threedee-lazyloaded-lg("dancer.png","imgs/threedee/dancer.png","","w-half","image")  
