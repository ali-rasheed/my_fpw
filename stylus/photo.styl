input[type=range].col-slider {
  -webkit-appearance: none;
  width: 100%;
  margin: 0.7px 0;
}
input[type=range].col-slider:focus {
  outline: none;
}
input[type=range].col-slider::-webkit-slider-runnable-track {
  width: 100%;
  height: 25.6px;
  cursor: pointer;
  box-shadow: 1px 1px 1px #000000, 0px 0px 1px #0d0d0d;
  background: #484d4d;
  border-radius: 0px;
  border: 0px solid #484d4d;
}
input[type=range].col-slider::-webkit-slider-thumb {
  box-shadow: 0px 0px 1px #ff435f, 0px 0px 0px #ff5d75;
  border: 0px solid #ff435f;
  height: 27px;
  width: 18px;
  border-radius: 0px;
  background: rgba(255, 67, 95, 0.77);
  cursor: pointer;
  -webkit-appearance: none;
  margin-top: -0.7px;
}
input[type=range].col-slider:focus::-webkit-slider-runnable-track {
  background: #c6c9c9;
}
input[type=range].col-slider::-moz-range-track {
  width: 100%;
  height: 25.6px;
  cursor: pointer;
  box-shadow: 1px 1px 1px #000000, 0px 0px 1px #0d0d0d;
  background: #484d4d;
  border-radius: 0px;
  border: 0px solid #484d4d;
}
input[type=range].col-slider::-moz-range-thumb {
  box-shadow: 0px 0px 1px #ff435f, 0px 0px 0px #ff5d75;
  border: 0px solid #ff435f;
  height: 27px;
  width: 18px;
  border-radius: 0px;
  background: rgba(255, 67, 95, 0.77);
  cursor: pointer;
}
input[type=range].col-slider::-ms-track {
  width: 100%;
  height: 25.6px;
  cursor: pointer;
  background: transparent;
  border-color: transparent;
  color: transparent;
}
input[type=range].col-slider::-ms-fill-lower {
  background: #000000;
  border: 0px solid #484d4d;
  border-radius: 0px;
  box-shadow: 1px 1px 1px #000000, 0px 0px 1px #0d0d0d;
}
input[type=range].col-slider::-ms-fill-upper {
  background: #484d4d;
  border: 0px solid #484d4d;
  border-radius: 0px;
  box-shadow: 1px 1px 1px #000000, 0px 0px 1px #0d0d0d;
}
input[type=range].col-slider::-ms-thumb {
  box-shadow: 0px 0px 1px #ff435f, 0px 0px 0px #ff5d75;
  border: 0px solid #ff435f;
  height: 27px;
  width: 18px;
  border-radius: 0px;
  background: rgba(255, 67, 95, 0.77);
  cursor: pointer;
  height: 25.6px;
}
input[type=range].col-slider:focus::-ms-fill-lower {
  background: #484d4d;
}
input[type=range].col-slider:focus::-ms-fill-upper {
  background: #c6c9c9;
}
